{
    "plugins": ["prettier"],
    "extends": ["react-app","react-app/jest","eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "ignorePatterns": ["node_modules/","build/"],
    "rules": {
        "prettier/prettier": "error",
        "no-var": "error",
        "no-unused-vars": "warn",
        "no-console": "warn"
    }
}
